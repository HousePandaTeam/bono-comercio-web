{
  "name": "bono-comercio-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Comercios adheridos al bono comercio de Valencia",
  "scripts": {
    "dev": "cd web && vite",
    "dev:lan": "cd web && vite --host 0.0.0.0",
    "generate-data": "node scripts/generate-data.js",
    "prebuild": "npm run generate-data",
    "build": "cd web && vite build",
    "build:github": "npm run generate-data && cd web && vite build",
    "preview": "cd web && vite preview",
    "preview:lan": "cd web && vite preview --host 0.0.0.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "leaflet": "^1.9.4",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.8",
    "eslint": "^8.57.0"
  }
}
